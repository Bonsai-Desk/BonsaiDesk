{
  "homepage": ".",
  "name": "BonsaiUI",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "lodash-es": "^4.17.20",
    "react": "^16.14.0",
    "react-dom": "^16.13.1",
    "react-player": "^2.7.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-spring": "^9.0.0-beta.32",
    "react-use-gesture": "^9.0.0-beta.11",
    "react-youtube": "^7.13.0",
    "tailwindcss": "^1.6.2"
  },
  "scripts": {
    "start": "npm run watch:css && react-scripts start",
    "build": "npm run build:css && react-scripts build && echo '/* /index.html 200' > build/_redirects",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "postcss src/tailwind.css -o src/assets/main.css",
    "watch:css": "postcss src/tailwind.css -o src/assets/main.css",
    "generate-c-sharp": "npm run build && node ./scripts/generateCSharp.js",
    "build-streaming": "npm run build && npm run clear-streaming-build && npm run copy-to-streaming",
    "clear-streaming-build": "rm -rf '../Assets/StreamingAssets/build'",
    "copy-to-streaming": "cp -r build '../Assets/StreamingAssets'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "postcss-cli": "^7.1.1",
    "cheerio": "^1.0.0-rc.2",
    "postcss-import": "^12.0.1"
  }
}
